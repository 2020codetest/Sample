{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "allowJs": true,
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "moduleResolution": "node",
        "module": "CommonJS",
        "strict": true,
        "removeComments": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_moduels"
    ]
}